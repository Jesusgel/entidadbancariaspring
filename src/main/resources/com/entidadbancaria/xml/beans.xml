<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id='banco' class="com.entidadbancaria.beans.Banco">
		<property name="banNom" value='Banco Provincial'></property>
	</bean>
	
	<bean id='cliente' class="com.entidadbancaria.beans.Cliente">
		<constructor-arg value="1"></constructor-arg>
		<constructor-arg value="StarLord"></constructor-arg>
		<constructor-arg value="21333555"></constructor-arg>
	</bean>

</beans>
